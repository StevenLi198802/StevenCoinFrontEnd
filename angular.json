{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "StevenCoinFrontEnd": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "baseHref": "",
            "outputPath": "dist/StevenCoinFrontEnd",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "zh-Hans": {
              "aot": true,
              "outputPath": "dist/StevenCoinFrontEnd-zh-Hans",
              "i18nFile": "src/i18n/messages.zh-Hans.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "zh-Hans"
            },
            "ja": {
              "aot": true,
              "outputPath": "dist/StevenCoinFrontEnd-ja",
              "i18nFile": "src/i18n/messages.ja.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "ja"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "StevenCoinFrontEnd:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "StevenCoinFrontEnd:build:production"
            },
            "zh-Hans": {
              "browserTarget": "StevenCoinFrontEnd:build:zh-Hans"
            },
            "ja": {
              "browserTarget": "StevenCoinFrontEnd:build:ja"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "StevenCoinFrontEnd:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "StevenCoinFrontEnd:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "StevenCoinFrontEnd:serve:production"
            }
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {
            "xliffmergeOptions": {
              "i18nFormat": "xlf",
              "srcDir": "src/i18n",
              "genDir": "src/i18n",
              "defaultLanguage": "en-US",
              "languages": [
                "en-US",
                "zh-Hans",
                "ja-JP"
              ]
            }
          }
        }
      }
    }
  },
  "defaultProject": "StevenCoinFrontEnd"
}